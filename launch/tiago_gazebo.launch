<?xml version="1.0" encoding="UTF-8"?>

<launch>
    <arg name="debug" default="false"/>
    <arg name="world" default="simple_office_with_people"/>
    <arg name="extra_gazebo_args" default=""/>

    <include file="$(find tiago_gazebo)/launch/tiago_gazebo.launch">
        <arg name="end_effector" value="pal-gripper"/>
        <arg name="gui" value="true"/>
        <arg name="debug" default="$(arg debug)"/>
        <arg name="public_sim" value="true"/>
        <arg name="tuck_arm" default="false"/>
        <arg name="base_type" value="pmb2"/>
        <arg name="world" default="$(arg world)"/>
        <arg name="extra_gazebo_args" value="$(arg extra_gazebo_args)"/>
    </include>

    <include file="$(find ros_tcp_endpoint)/launch/endpoint.launch">
    </include>

    <include file="$(find tiago_telepresence_controllers)/launch/telepresence_controllers.launch">
    </include>
</launch>
